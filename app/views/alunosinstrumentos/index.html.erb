<p id="notice"><%= notice %></p>

<%= link_to 'Professor', professors_path %>
<%= link_to 'Aluno', alunos_path %>
<%= link_to 'Instrumento', instrumentos_path %>

<%= link_to image_tag("Escola de musica.png"), professors_path %>

<h1>Alunos_instrumentos</h1>

<table>
  <thead>
    <tr>
      <th>Aluno</th>
      <th>Instrumento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunosinstrumentos.each do |alunosinstrumento| %>
      <tr>
        <td><%= alunosinstrumento.aluno.nome %></td>
        <td><%= alunosinstrumento.instrumento.nome %></td>
        <td><%= link_to 'Show', alunosinstrumento %></td>
        <td><%= link_to 'Edit', edit_alunosinstrumento_path(alunosinstrumento) %></td>
        <td><%= link_to 'Destroy', alunosinstrumento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alunosinstrumento', new_alunosinstrumento_path %>
