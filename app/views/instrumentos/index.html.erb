<p id="notice"><%= notice %></p>

<%= link_to 'Professor', professors_path %>
<%= link_to 'Aluno', alunos_path %>
<%= link_to 'Alunos Instrumentos', alunosinstrumentos_path %>

<%= link_to image_tag("Escola de musica.png"), professors_path %>

<h1>Instrumento</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instrumentos.each do |instrumento| %>
      <tr>
        <td><%= instrumento.id %></td>
        <td><%= instrumento.nome %></td>
        <td><%= link_to 'Show', instrumento %></td>
        <td><%= link_to 'Edit', edit_instrumento_path(instrumento) %></td>
        <td><%= link_to 'Destroy', instrumento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Instrumento', new_instrumento_path %>
